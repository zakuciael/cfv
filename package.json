{
  "name": "cfv",
  "version": "1.2.0",
  "description": "CFV is a cross-platform node.js library that can extract the file version from a valid PE file (.exe)",
  "main": "dist/index.js",
  "repository": "https://github.com/zakuciael/cfv.git",
  "author": "Krzysztof Saczuk <zakuciael@outlook.com>",
  "license": "MIT",
  "private": false,
  "types": "./index.d.ts",
  "keywords": [
    "cross-platform",
    "file-version",
    "file",
    "info"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src/**/*.ts",
    "prepublish": "yarn lint && yarn build"
  },
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
